#include<iostream>
#include<vector>
using namespace std;

struct song{
  string title;
  string artist;
  int year;
  string producer;
};

int main(){

vector<song>mySong;
  song song1, song2;
  
  song1.title="fvdsf";
  song1.artist="csdc";
  song1.year=2005;
  song2.title="fv";
  song2.year=4334;
  mySong.push_back(song1);
  mySong.push_back(song2);

  for(auto & x:mySong){
    cout<<x.title<<"by"<<x.artist<<" was releasd"<<x.year<<endl;
  }

  //for( auto itr=mySong.begin(); itr!=mySong.end; itr++)
  //cout<<*itr<<endl;

return 0;
}


